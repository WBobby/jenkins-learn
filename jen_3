pipeline{

    agent {
        label 'xxx-agent-machine'
        customWorkspace "${env.JOB_NAME}/${env.BUILD_NUMBER}"
    }

    stages{
        stage ('Build'){
            println 'Build Start...'
            sh 'ls -al'
        }

        stage ('Test'){
            println 'Test start...'
            sh "echo ${JAVA_HOME}"
        }
    }
}